{
  "name": "components-library-io",
  "version": "0.2.20",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "typings": "lib/index.d.ts",
  "private": false,
  "files": [
    "lib",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/isaozler/components-library-io.git"
  },
  "dependencies": {
    "@mdi/js": "^6.4.95",
    "@mdi/react": "^1.5.0",
    "dayjs": "^1.10.7"
